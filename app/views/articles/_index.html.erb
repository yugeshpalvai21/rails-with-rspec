<h1 align="center">All Articles</h1>
<ul>
<% @articles.each do |article| %>
    <li><%= article.title %><li>
<% end %>
</ul>

<hr>

<%= form_with model: @article do |form| %>
    <div class="form-group">
        <%= form.label :title %>
        <%= form.text_field :title %>
    </div>
    <div class="form-group">
        <%= form.submit "Create Article" %>
    </div>
<% end %>